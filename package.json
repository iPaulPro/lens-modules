{
  "name": "lens-modules",
  "version": "3.0.1",
  "description": "Contracts, ABIs, and utilities for Lens Protocol.",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/contracts/**/*.sol",
    "dist"
  ],
  "exports": {
    ".": {
      "import":   "./dist/index.js",
      "types":    "./dist/index.d.ts"
    },
    "./abis": {
      "import":   "./dist/abis.js",
      "types":    "./dist/abis.d.ts"
    },
    "./deployments": {
      "import":   "./dist/deployments.js",
      "types":    "./dist/deployments.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "abis":      ["dist/abis.d.ts"],
      "deployments": ["dist/deployments.d.ts"]
    }
  },
  "scripts": {
    "build": "tsc",
    "prepublish": "tsc",
    "generate": "npx wagmi generate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iPaulPro/lens-modules.git"
  },
  "keywords": [
    "lens-protocol",
    "lens",
    "solidity",
    "ethereum",
    "smart",
    "contracts",
    "web3-social",
    "web3"
  ],
  "author": "Paul Burke <paul@paulburke.co>",
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@openzeppelin/contracts": "4.9.6",
    "@types/node": "^22.15.21",
    "@wagmi/cli": "^2.3.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-prettier": "^5.4.0",
    "prettier": "^3.5.3",
    "typescript": "^5.4.2",
    "viem": "^2.30.0"
  }
}
